<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
<listener>
	<listener-class>web.CountListener</listener-class>
</listener>
  <filter>
    <filter-name>loginVerify</filter-name>
    <filter-class>web.LoginVerifyFilter</filter-class>
  </filter>
     <filter>
    <filter-name>CheckcodeVerifyFilter</filter-name>
    <filter-class>web.CheckcodeVerifyFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>loginVerify</filter-name>
    <url-pattern>*.do</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>CheckcodeVerifyFilter</filter-name>
    <url-pattern>/regist.do</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>CheckcodeVerifyFilter</filter-name>
    <url-pattern>/login.log</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>ActionServlet</servlet-name>
    <servlet-class>web.ActionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ActionServlet</servlet-name>
    <url-pattern>*.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ActionServlet</servlet-name>
    <url-pattern>/toRegist.s</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ActionServlet</servlet-name>
    <url-pattern>/toLogin.s</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ActionServlet</servlet-name>
    <url-pattern>/login.s</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    <servlet-name>ActionServlet</servlet-name>
    <url-pattern>/regist.s</url-pattern>
  </servlet-mapping>
    	<servlet>
		<servlet-name>index</servlet-name>
		<servlet-class>web.ActionServlet</servlet-class>
	</servlet>
  <servlet-mapping>
		<servlet-name>index</servlet-name>
		<url-pattern>/index.html</url-pattern>
	</servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CheckcodeServlet</display-name>
    <servlet-name>CheckcodeServlet</servlet-name>
    <servlet-class>web.CheckcodeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CheckcodeServlet</servlet-name>
    <url-pattern>/checkcode</url-pattern>
  </servlet-mapping>
	


</web-app>